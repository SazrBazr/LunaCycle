{
  "name": "Period App",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "service_worker.js"
  },
  "display": "standalone",
  "background_color": "#f7f7f7",
  "theme_color": "#ff4081",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["content.js"]
    }
  ],
  "icons": [
      {
          "src": "icons/AppIcon128.png",
          "sizes": "128x128",
          "type": "image/png"
      },
      {
          "src": "icons/AppIcon512.png",
          "sizes": "512x512",
          "type": "image/png"
      }
  ]
}